{
    "name": "bishopb/pattern",
    "description": "A string-matching PHP library sporting a consistent, fluent API.",
    "license": "MIT",
    "authors": [
        { "name": "Bishop Bettini", "email": "bishop@php.net" }
    ],
    "autoload": {
        "classmap": [ "src" ],
        "psr-4": {
            "BishopB\\Pattern\\": "src/",
            "BishopB\\Pattern\\Exception\\": "src/exceptions/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "athletic/athletic": "~0.1"
    },
    "autoload-dev": {
        "psr-4": {
            "BishopB\\Pattern\\": "tests/"
        }
    },
    "require": {
        "phpdocumentor/phpdocumentor": "~2.0"
    }
}
