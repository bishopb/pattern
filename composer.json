{
    "name": "bishopb/stencil",
    "description": "A PHP library for applying patterns to subject strings using a consistent, fluent API.",
    "license": "MIT",
    "authors": [
        { "name": "Bishop Bettini", "email": "bishop@php.net" }
    ],
    "autoload": {
        "classmap": [ "src" ],
        "psr-4": {
            "BishopB\\Stencil\\": "src/",
            "BishopB\\Stencil\\Exception\\": "src/exceptions/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "athletic/athletic": "~0.1"
    },
    "autoload-dev": {
        "psr-4": {
            "BishopB\\Stencil\\": "tests/"
        }
    }
}
