{
    "name": "bishopb/pattern",
    "description": "A string-matching PHP library sporting a consistent, fluent API.",
    "license": "MIT",
    "authors": [
        { "name": "Bishop Bettini", "email": "bishop@php.net", "role": "lead" }
    ],
    "require": {
        "php": "~5.3"
    },
    "autoload": {
        "classmap": [ "src" ],
        "psr-4": {
            "BishopB\\Pattern\\": "src/",
            "BishopB\\Pattern\\Exception\\": "src/exceptions/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "0.9.4",
        "athletic/athletic": "~0.1",
        "phpunit/test-listener-xhprof": "1.0.*@dev",
        "facebook/xhprof": "dev-master@dev",
        "phpunit/php-timer": "~1.0"
    },
    "autoload-dev": {
        "psr-4": {
            "BishopB\\Pattern\\": "tests/"
        }
    }
}
